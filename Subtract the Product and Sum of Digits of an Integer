class Solution {
    public int subtractProductAndSum(int n) {
        int k=n;
            int sum =0;
            int pro=1;
        while(k>0){
            int rem=k%10;
            sum=sum+rem;
            pro=pro*rem;
            k=k/10;
        }
        int result=pro-sum;
        return result;
    }
}
